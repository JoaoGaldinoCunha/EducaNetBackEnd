package br.com.school.educanet.model;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;
@Entity
@Table(name = "tb_course")
public class TbCourse {
	
	@Id
	private long  courseId;
	private String courseName;
	private Integer workload;
	private String description;
	private String courseClass;
	
	
	
	public Long getCouserId() {
		return courseId;
	}
	public void setCouserId(Long courseId) {
		this.courseId = courseId;
	}
	public String getCouserName() {
		return courseName;
	}
	public void setCouserName(String couserName) {
		this.courseName = couserName;
	}
	public Integer getWorkload() {
		return workload;
	}
	public void setWorkload(Integer workload) {
		this.workload = workload;
	}
	public String getDescrpition() {
		return description;
	}
	public void setDescrpition(String description) {
		this.description = description;
	}
	public String getCourseClass() {
		return courseClass;
	}
	public void setCourseClass(String courseClass) {
		this.courseClass = courseClass;
	}
}
	
	